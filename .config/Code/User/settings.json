{
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "editor.defaultFormatter": null,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoIndent": "keep",
  "editor.comments.ignoreEmptyLines": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorSurroundingLines": 16,
  "editor.dragAndDrop": false,
  "editor.folding": false,
  "editor.fontFamily": "'Iosevka Expanded', 'Comic Shanns Regular', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.language.colorizedBracketPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["»", "«"],
    ["›", "‹"]
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.unicodeHighlight.includeComments": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.sortOrder": "alphabetical",
  "files.associations": {
    "*.hcl": "terraform"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autoRepositoryDetection": "openEditors",
  "git.confirmSync": false,
  "git.countBadge": "tracked",
  "git.ignoredRepositories": ["/home/main"],
  "haskell.formattingProvider": "stylish-haskell",
  "haskell.plugin.ghcide-completions.config.autoExtendOn": false,
  "haskell.plugin.importLens.codeActionsOn": false,
  "haskell.plugin.importLens.codeLensOn": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "svg.preview.autoShow": true,
  "svg.preview.mode": "svg",
  "svg.preview.viewMode": "oneByOne",
  "telemetry.telemetryLevel": "off",
  "terraform.codelens.referenceCount": true,
  "window.menuBarVisibility": "toggle",
  "window.title": "${folderName}/${activeEditorMedium}",
  "workbench.colorCustomizations": {
    "comment": "See https://code.visualstudio.com/api/references/theme-color",
    "editor.selectionBackground": "#f0d9ae",
    "editor.selectionHighlightBorder": "#5b8b95",
    "editorBracketHighlight.foreground1": "#5d81b4",
    "editorBracketHighlight.foreground2": "#eb6235",
    "editorBracketHighlight.foreground3": "#8eb031",
    "editorBracketHighlight.foreground4": "#e09b24",
    "editorBracketHighlight.foreground5": "#8678b2",
    "editorBracketHighlight.foreground6": "#c46e1a",
    "editorBracketHighlight.unexpectedBracket.foreground": "#f700ff",
    "editorInlayHint.foreground": "#949494",
    "editorInlayHint.background": "#00000000",
    "editorInlayHint.typeForeground": "#949494",
    "editorInlayHint.typeBackground": "#00000000",
    "editorInlayHint.parameterForeground": "#949494",
    "editorInlayHint.parameterBackground": "#00000000"
  },
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 8,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.enableExperiments": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 24,
  "editor.autoClosingOvertype": "never",
  "editor.fastScrollSensitivity": 10,
  "editor.glyphMargin": true,
  "editor.mouseWheelZoom": true,
  "editor.renderLineHighlight": "all",
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.find.cursorMoveOnType": true,
  "editor.minimap.showSlider": "always",
  "editor.suggest.showStatusBar": true,
  "workbench.list.smoothScrolling": true,
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.autoLockGroups": {},
  "window.enableMenuBarMnemonics": false,
  "explorer.decorations.badges": false,
  "update.mode": "manual",
  "git.confirmForcePush": false,
  "terraform.languageServer.enable": true,
  "terraform.languageServer.path": "",
  "editor.guides.bracketPairs": true,
  "typescript.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.inputValidationSubjectLength": 9000,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "editor.inlayHints.fontSize": 10,
  "playwright.reuseBrowser": true,
  "extensions.ignoreRecommendations": true,
  "jest.autoRun": {},
  "search.smartCase": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "workbench.editor.tabActionCloseVisibility": false,
  "javascript.updateImportsOnFileMove.enabled": "always"
}
