{
    "[haskell]": {
        "editor.defaultFormatter": "haskell.haskell"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "keep",
    "editor.comments.ignoreEmptyLines": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorSurroundingLines": 5,
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.folding": false,
    "editor.fontFamily": "'Iosevka Expanded', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": false,
    "editor.language.colorizedBracketPairs": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"],
        ["»", "«"],
        ["›", "‹"]
    ],
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.unicodeHighlight.includeComments": true,
    "editor.unicodeHighlight.nonBasicASCII": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.sortOrder": "alphabetical",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.sonnet": "jsonnet"
    },
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gcode.general.statusBars.enabled": false,
    "gcode.general.units": "Metric",
    "git.autoRepositoryDetection": "openEditors",
    "git.confirmSync": false,
    "git.countBadge": "tracked",
    "git.ignoredRepositories": ["/home/main"],
    "haskell.formattingProvider": "stylish-haskell",
    "haskell.plugin.ghcide-completions.config.autoExtendOn": false,
    "haskell.plugin.importLens.codeActionsOn": false,
    "haskell.plugin.importLens.codeLensOn": false,
    "haskell.plugin.refineImports.globalOn": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.pythonPath": "/usr/bin/python3",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "svg.preview.autoShow": true,
    "svg.preview.mode": "svg",
    "svg.preview.viewMode": "oneByOne",
    "telemetry.telemetryLevel": "off",
    "terraform.codelens.referenceCount": true,
    "terraform.languageServer": {
        "args": ["serve"],
        "external": true,
        "pathToBinary": "",
        "trace.server": "off"
    },
    "update.mode": "manual",
    "window.menuBarVisibility": "toggle",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.zoomLevel": 1,
    "workbench.colorCustomizations": {
        "comment": "See https://code.visualstudio.com/api/references/theme-color",
        "editor.selectionBackground": "#f0d9ae",
        "editor.selectionHighlightBorder": "#5b8b95",
        "editorBracketHighlight.foreground1": "#5d81b4",
        "editorBracketHighlight.foreground2": "#eb6235",
        "editorBracketHighlight.foreground3": "#8eb031",
        "editorBracketHighlight.foreground4": "#e09b24",
        "editorBracketHighlight.foreground5": "#8678b2",
        "editorBracketHighlight.foreground6": "#c46e1a",
        "editorBracketHighlight.unexpectedBracket.foreground": "#f700ff",
    },
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 8,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 24
}
