{
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "keep",
    "editor.comments.ignoreEmptyLines": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorSurroundingLines": 5,
    "editor.dragAndDrop": false,
    "editor.folding": false,
    "editor.fontFamily": "'Iosevka Expanded', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.sortOrder": "alphabetical",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.sonnet": "jsonnet"
    },
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autoRepositoryDetection": "openEditors",
    "git.confirmSync": false,
    "git.countBadge": "tracked",
    "git.ignoredRepositories": [
        "/home/main"
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.pythonPath": "/usr/bin/python3",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terraform.languageServer": {
        "external": true,
        "pathToBinary": "",
        "args": [
            "serve"
        ],
        "maxNumberOfProblems": 100,
        "trace.server": "off"
    },
    "window.menuBarVisibility": "toggle",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 8,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 24,
    "haskell.plugin.hlint.diagnosticsOn": false,
    "haskell.plugin.ghcide-completions.config.autoExtendOn": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "svg.preview.mode": "svg",
    "update.mode": "manual",
    "haskell.plugin.importLens.codeLensOn": false,
    "haskell.plugin.refineImports.globalOn": false,
    "haskell.plugin.importLens.codeActionsOn": false,
    "terraform.codelens.referenceCount": true,
    "haskell.formattingProvider": "stylish-haskell"
}
